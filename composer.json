{

  "name": "serps/cli",
  "description": "Command line utility to scrape from the terminal",
  "type": "project",
  "keywords": ["SERPS", "Google"],
  "homepage": "https://github.com/serp-spider/serps-cli",
  "license": "Fair",

  "authors": [
    {
      "name": "Soufiane GHZAL",
      "homepage": "https://github.com/gsouf"
    }
  ],

  "autoload":{
    "psr-4" : {
      "SerpsCli\\"   :   "src/"
    }
  },

  "bin": [
    "bin/serps"
  ],

  "require": {
    "php": ">=5.5.9",
    "corneltek/cliframework": "~3.0",

    "serps/core": "dev-master",
    "serps/search-engine-google": "dev-master",

    "serps/http-client-phantomjs": "dev-master",
    "serps/http-client-spidyjs": "dev-master",
    "serps/http-client-curl": "dev-master"
  },

  "require-dev":{
    "squizlabs/php_codesniffer": "~2.5"
  },

  "scripts": {
      "csfix": "yes 2>/dev/null | test/bin/phpcbf.bash",
      "cscheck": "test/bin/phpcs.bash emacs"
  }
}
